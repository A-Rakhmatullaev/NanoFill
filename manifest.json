{
    "manifest_version": 3,
    "name": "NanoFill",
    "version": "1.0",
    "description": "Gemini Nano powered alternative to Google Autofill",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      }
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "css": ["content_script.css"]
    }],
    "permissions": [
        "storage",
        "activeTab"
    ]
  }
  